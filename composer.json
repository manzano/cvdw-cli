{
  "name": "manzano/cvdw-cli",
  "description": "Suite para buscar dados da API do CV CVDW para um banco de dados",
  "homepage": "https://manzano.me",
  "license": "MIT",
  "minimum-stability": "stable",
  "type": "library",
  "require": {
    "php": "^8.0",
    "symfony/finder": "^6.3",
    "doctrine/dbal": "^3.7",
    "doctrine/migrations": "^3.6",
    "symfony/console": "^6.3",
    "symfony/dotenv": "^6.3",
    "symfony/yaml": "^6.3",
    "doctrine/doctrine-migrations-bundle": "^3.2",
    "doctrine/sql-formatter": "^1.1",
    "sentry/sentry": "^4.6",
    "mixpanel/mixpanel-php": "^2.11"
  },
  "require-dev": {
    "phpstan/phpstan": "^1.10",
    "phpunit/phpunit": "^11",
    "codeception/codeception": "^5.1",
    "codeception/module-rest": "^3.3",
    "codeception/module-phpbrowser": "^3.0",
    "codeception/module-asserts": "*"
  },
  "autoload": {
    "psr-4": {
      "Manzano\\CvdwCli\\": "src/app/"
      
    },
    "files": [
      "src/app/Inc/Objetos.php",
      "src/app/Inc/Helper.php",
      "src/app/Inc/Conexao.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/",
      "Support\\": "tests/Support/",
      "Helper\\": "tests/Support/Helper/"
    }
  },
  "scripts": {
    "test": [
      "@clearCache",
      "phpunit"
    ],
    "clearCache": "rm -rf .phpunit.cache"
  },
  "bin": [
    "app"
  ],
  "authors": [
    {
      "name": "Gabriel Manzano",
      "email": "gabriel@cvcrm.com.br"
    }
  ]
}